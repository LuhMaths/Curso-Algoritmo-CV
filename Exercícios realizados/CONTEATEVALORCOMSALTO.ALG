algoritmo "conteAteValorComSalto"

var
   valor, contador, salto : inteiro
inicio
   contador <- 0
   Escreva ("Quer contar até quanto? ")
   Leia (valor)
   Escreva ("Qual o valor do salto? ")
   Leia (salto)
   Enquanto (contador <= valor) faca
            Escreval (contador)
            contador <- contador + salto
   FimEnquanto
   Escreval ("Terminei de contar")
fimalgoritmo