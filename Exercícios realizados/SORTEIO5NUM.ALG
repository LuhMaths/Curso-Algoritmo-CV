algoritmo "Sorteio5num"

var
  num, n : inteiro
inicio
    Escreva ("Digite um número: ")
    Leia (n)
    Escreval ("Número sorteado: ")
    aleatorio 1, 5
    Leia (num)
    aleatorio off
    Se (num = n) entao
       Escreval ("Você acertou o número sorteado")
    senao
         Escreval ("Você errou")
    FimSe
fimalgoritmo